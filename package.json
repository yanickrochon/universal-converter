{
  "name": "universal-converter",
  "version": "1.5.2",
  "description": "Convert just about anything to anything else.",
  "author": "Yanick Rochon <yanick.rochon@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "browser": "dist/universal-converter.min.js",
  "files": [
    "lib",
    "definitions"
  ],
  "scripts": {
    "test": "jest --forceExit",
    "test-cov": "npm run test -- --coverage",
    "lint": "eslint benchmarks src --fix",
    "prepublishOnly": "gulp"
  },
  "homepage": "https://github.com/yanickrochon/universal-converter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yanickrochon/universal-converter.git"
  },
  "bugs": {
    "url": "https://github.com/yanickrochon/universal-converter/issues"
  },
  "keywords": [
    "unit",
    "converter",
    "universal",
    "conversion",
    "math",
    "physics"
  ],
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "babelify": "^10.0.0",
    "gulp": "^4.0.2",
    "gulp-browserify": "^0.5.1",
    "gulp-uglify": "^3.0.2",
    "jest": "^24.8.0"
  },
  "engines": {
    "node": ">= 8"
  },
  "jest": {
    "verbose": false,
    "testMatch": [
      "**/test/**/*.js"
    ],
    "coverageReporters": [
      "text-summary",
      "lcov"
    ],
    "bail": false,
    "testEnvironment": "node"
  },
  "dependencies": {
    "@babel/preset-env": "^7.4.5",
    "gulp-rename": "^1.4.0",
    "gulp-sourcemaps": "^2.6.5"
  }
