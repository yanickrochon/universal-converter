{
  "name": "universal-converter",
  "version": "1.3.0",
  "description": "Convert just about anything to anything else.",
  "author": "Yanick Rochon <yanick.rochon@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "browser": "dist/universal-converter.min.js",
  "files": [
    "lib",
    "definitions"
  ],
  "scripts": {
    "test": "jest --forceExit",
    "test-cov": "npm run test -- --coverage",
    "lint": "eslint benchmarks src --fix",
    "prepublishOnly": "gulp"
  },
  "homepage": "https://github.com/yanickrochon/universal-converter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yanickrochon/universal-converter.git"
  },
  "bugs": {
    "url": "https://github.com/yanickrochon/universal-converter/issues"
  },
  "keywords": [
    "unit",
    "converter",
    "universal",
    "conversion",
    "math",
    "physics"
  ],
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "eslint": "^4.8.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "gulp": "^3.9.1",
    "gulp-buffer": "0.0.2",
    "gulp-derequire": "^2.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-uglify": "^3.0.0",
    "gulp-util": "^3.0.8",
    "istanbul": "^0.4.5",
    "jest": "^21.2.1",
    "uppercamelcase": "^3.0.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "engines": {
    "node": ">= 4"
  },
  "jest": {
    "verbose": false,
    "testMatch": [
      "**/test/**/*.js"
    ],
    "coverageReporters": [
      "text-summary",
      "lcov"
    ],
    "bail": false,
    "testEnvironment": "node"
  }
}
